{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch with Node v20.17.0",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/app.ts",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v20.17.0/bin/node"
    }
  ]
}
